{
    "format_version": "1.16.0",
    "minecraft:entity": {
        "component_groups": {
            "emerging": {
                "minecraft:behavior.emerge": {
                    "duration": 1,
                    "on_done": {
                        "event": "minecraft:emerged",
                        "target": "self"
                    }
                }
            },
            "pushable": {
                "minecraft:pushable": {
                    "is_pushable": true,
                    "is_pushable_by_piston": true
                }
            }
        },
        "components": {
            "minecraft:behavior.dig": {
                "digs_in_daylight": false,
                "duration": 5.5,
                "idle_time": 500,
                "on_start": {
                    "event": "on_digging_event",
                    "target": "self"
                },
                "priority": 1,
                "suspicion_is_disturbance": true,
                "vibration_is_disturbance": true
            },
            "minecraft:behavior.eat_block": {
                "eat_and_replace_block_pairs": [
                    {
                        "eat_block": "grass",
                        "replace_block": "dirt"
                    },
                    {
                        "eat_block": "tallgrass",
                        "replace_block": "air"
                    }
                ],
                // "on_eat": {
                //     "event": "minecraft:on_eat_block",
                //     "target": "self"
                // },
                "priority": 15,
                "success_chance": "0.01",
                "time_until_eat": 1.8
            },
            "minecraft:behavior.eat_mob": {
                "eat_animation_time": 0.3,
                "eat_mob_sound": "tongue",
                "loot_table": "loot_tables/entities/frog.json",
                "priority": 8,
                "pull_in_force": 0.75,
                "reach_mob_distance": 1.75,
                "run_speed": 0.5
            },
            "minecraft:behavior.float": {
                "priority": 0
            },
            "minecraft:behavior.nearest_attackable_target": {
                "entity_types": [
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "subject": "other",
                                    "test": "is_family",
                                    "value": "bee"
                                }
                            ]
                        },
                        "max_dist": 16
                    }
                ],
                "must_see": true,
                "priority": 7,
                "within_radius": 16
            },
            "minecraft:behavior.random_look_around": {
                "priority": 11
            },
            "minecraft:behavior.random_stroll": {
                "priority": 9,
                "speed_multiplier": 0.5
            },
            "minecraft:breathable": {
                "suffocate_time": 0,
                "total_supply": 15
            },
            "minecraft:can_climb": {},
            "minecraft:collision_box": {
                "height": 2.9,
                "width": 0.9
            },
            "minecraft:experience_reward": {
                "on_death": "query.last_hit_by_player ? 5 : 0"
            },
            "minecraft:fire_immune": {},
            "minecraft:follow_range": 30,
            "minecraft:health": {
                "max": 500,
                "value": 500
            },
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:jump.static": {},
            "minecraft:knockback_resistance": {
                "value": 1
            },
            "minecraft:loot": {
                "table": "loot_tables/entities/warden.json"
            },
            "minecraft:movement": {
                "value": 0.3
            },
            "minecraft:movement.basic": {},
            "minecraft:nameable": {},
            "minecraft:navigation.walk": {
                "avoid_damage_blocks": true,
                "can_path_over_lava": true,
                "can_path_over_water": true
            },
            "minecraft:physics": {},
            "minecraft:type_family": {
                "family": [
                    "great_hunger",
                    "monster",
                    "mob"
                ]
            }
        },
        "description": {
            "identifier": "smokey_bedrock:great_hunger",
            "is_experimental": false,
            "is_spawnable": true,
            "is_summonable": true
        },
        "events": {
            "minecraft:emerged": {
                "add": {
                    "component_groups": [
                        "pushable"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "emerging"
                    ]
                }
            },
            "minecraft:entity_spawned": {
                "add": {
                    "component_groups": [
                        "pushable"
                    ]
                }
            },
            "minecraft:spawn_emerging": {
                "add": {
                    "component_groups": [
                        "emerging"
                    ]
                }
            },
            "on_digging_event": {
                "remove": {
                    "component_groups": [
                        "pushable"
                    ]
                }
            }
        }
    }
}