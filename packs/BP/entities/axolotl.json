            "axolotl_gold": {
                "minecraft:variant": {
                    "value": 2
                }
            },
            "axolotl_in_water": {
                "minecraft:environment_sensor": {
                    "triggers": [
                        {
                            "event": "start_drying_out",
                            "filters": {
                                "operator": "!=",
                                "test": "in_water",
                                "value": true
                            }
                        }
                    ]
                }
            },
            "axolotl_lucy": {
                "minecraft:variant": {
                    "value": 0
                }
            },
