{
    "format_version": "1.16.0",
    "minecraft:entity": {
        "component_groups": {
            "smokey_bedrock:convert_to_warden": {
                "minecraft:transformation": {
                    "into": "minecraft:warden<minecraft:spawn_emerging>",
                    "transformation_sound": "convert_to_drowned",
                    "drop_equipment": true,
                    "delay": {
                        "value": 4
                    }
                },
                "minecraft:is_shaking": {}
            },
            "attack_cooldown": {
